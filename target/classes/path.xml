<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="hi" class="org.example.Hai">
        <property name="id" value="10"/>
        <property name="name" value="Puma"/>
        <property name="price" value="1200"/>
    </bean>
    <bean id="hii" class="org.example.Hai">
        <constructor-arg index="0" value="201"/>
        <constructor-arg index="1" value="boat"/>
        <constructor-arg index="2" value="3400"/>
    </bean>
</beans>